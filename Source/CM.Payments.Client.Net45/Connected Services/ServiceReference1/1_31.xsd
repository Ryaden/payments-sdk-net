            <extension base="string">
              <attribute name="score" type="int" use="required" />
            </extension>
          </simpleContent>
        </complexType>
      </element>
      <any minOccurs="0" maxOccurs="unbounded" namespace="##local" />
    </sequence>
    <attribute name="score" type="int" use="required" />
