        <element minOccurs="1" maxOccurs="1" name="misterCashPaymentInfo" type="ddp:misterCashPaymentInfo" />
        <element minOccurs="1" maxOccurs="1" name="giftCardPaymentInfo" type="ddp:giftCardPaymentInfo" />
        <element minOccurs="1" maxOccurs="1" name="iDealPaymentInfo" type="ddp:iDealPaymentInfo" />
