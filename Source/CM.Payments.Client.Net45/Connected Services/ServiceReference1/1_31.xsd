      <element minOccurs="1" maxOccurs="1" name="parameters" type="ddp:parameters" />
      <any minOccurs="0" maxOccurs="unbounded" namespace="##local" />
    </sequence>
  </complexType>
  <complexType name="shopperInfo">
    <annotation>
      <documentation>Shopper information needed by 3DS.</documentation>
    </annotation>
    <sequence>
      <element minOccurs="1" maxOccurs="1" name="shopperIp" type="ddp:string250" />
      <element minOccurs="1" maxOccurs="1" name="browserAccept" type="ddp:string250" />
      <element minOccurs="1" maxOccurs="1" name="browserUserAgent" type="ddp:string250" />
    </sequence>
  </complexType>
</schema>