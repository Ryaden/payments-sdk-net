        <element minOccurs="1" maxOccurs="1" name="klarnaAccountPaymentInfo" type="ddp:openInvoicePaymentInfo" />
        <element minOccurs="1" maxOccurs="1" name="elvPaymentInfo" type="ddp:elvPaymentInfo" />
        <element minOccurs="1" maxOccurs="1" name="sepaDirectDebitPaymentInfo" type="ddp:sepaDirectDebitPaymentInfo" />
        <element minOccurs="1" maxOccurs="1" name="pointOfSalePaymentInfo" type="ddp:pointOfSalePaymentInfo" />
        <element minOccurs="1" maxOccurs="1" name="offlinePaymentInfo" type="ddp:offlinePaymentInfo" />
        <element minOccurs="1" maxOccurs="1" name="sofortUeberweisungPaymentInfo" type="ddp:sofortPaymentInfo" />
        <element minOccurs="1" maxOccurs="1" name="eBankingPaymentInfo" type="ddp:sofortPaymentInfo" />
        <element minOccurs="1" maxOccurs="1" name="systemPaymentInfo" type="ddp:systemPaymentInfo" />
      </choice>
      <any minOccurs="0" maxOccurs="unbounded" namespace="##local" />
    </sequence>
