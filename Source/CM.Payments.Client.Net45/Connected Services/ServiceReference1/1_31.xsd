        <element minOccurs="1" maxOccurs="1" name="pointOfSalePaymentInput" type="ddp:pointOfSalePaymentInput" />
        <element minOccurs="1" maxOccurs="1" name="offlinePaymentInput" type="ddp:offlinePaymentInput" />
        <element minOccurs="1" maxOccurs="1" name="elvPaymentInput" type="ddp:elvPaymentInput" />
        <element minOccurs="1" maxOccurs="1" name="iDealPaymentInput" type="ddp:iDealPaymentInput" />
